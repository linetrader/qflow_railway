{
  "name": "qflow",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "prestart": "prisma migrate deploy && npm run seed:guard",
    "start": "npm run start:all",
    "start:all": "bash ./bat/start-all.sh",
    "start:web": "next start -p ${PORT:-3000}",
    "dev:all": "bash ./bat/dev-all.sh",
    "seed:guard": "node prisma/seed.guard.mjs && bash ./bat/seed-all.sh || echo \"Seed already applied â€” skipping\"",
    "start:runner:mining": "tsx src/worker/mining/runner.ts",
    "start:runner:level": "tsx src/worker/level-recalc/workers/runner.ts",
    "start:runner:sweep": "tsx src/worker/sweep/runner.ts",
    "dev:runner:mining": "tsx watch src/worker/mining/runner.ts",
    "dev:runner:mining:once": "tsx src/worker/mining/runner.ts --once",
    "dev:runner:level": "tsx watch src/worker/level-recalc/workers/runner.ts",
    "dev:runner:level:once": "tsx src/worker/level-recalc/workers/runner.ts --once",
    "dev:runner:sweep": "tsx src/worker/sweep/runner.ts",
    "dev:runner:sweep:watch": "tsx watch src/worker/sweep/runner.ts",
    "dev:runner:sweep:once": "tsx src/worker/sweep/runner.ts --once",
    "lint": "next lint --quiet",
    "lint:all": "eslint --quiet .",
    "db:gen": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "prisma:fix": "rm -rf node_modules/.prisma && prisma generate"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@heroicons/react": "^2.2.0",
    "@noble/hashes": "^2.0.0",
    "@prisma/client": "^6.16.1",
    "@tiptap/extension-image": "^3.7.2",
    "@tiptap/extension-link": "^3.7.2",
    "@tiptap/pm": "^3.7.2",
    "@tiptap/react": "^3.7.2",
    "@tiptap/starter-kit": "^3.7.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.18",
    "ethers": "^6.15.0",
    "i18n-iso-countries": "^7.14.0",
    "jose": "^6.1.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.2",
    "next-intl": "^4.4.0",
    "otpauth": "^9.4.1",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-d3-tree": "^3.6.6",
    "react-dom": "19.1.0",
    "tsx": "^4.20.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.19.14",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "daisyui": "^5.1.27",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prisma": "^6.16.1",
    "sass": "^1.93.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
